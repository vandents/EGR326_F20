/**
 * Keypad, pushbuttons.
 *	Press (1 - 9) for steps * 4
 * 	Increment and decrement button
 * 	Print current step
 */

#include <stdio.h>
#include "msp.h"
#include "SysTick.h"
#include "Clock_48MHz.h"


/**
 * main.c
 */
void main(void) {
	WDT_A->CTL = WDT_A_CTL_PW | WDT_A_CTL_HOLD;		// stop watchdog timer
}
